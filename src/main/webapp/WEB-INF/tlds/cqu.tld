                <name>name</name>    
                <required>true</required>    
                <rtexprvalue>false</rtexprvalue>    
            </attribute>
</tag>  
 <tag>  
	 <name>userSelector</name>                             
	 <tag-class>com.yuqincar.tags.UserSelectorTags</tag-class> 
