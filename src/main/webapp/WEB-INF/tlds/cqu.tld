                <required>true</required>    
                <rtexprvalue>false</rtexprvalue>    
      </attribute> 
	 <attribute>    
                <name>synchDriver</name>    
                <required>false</required>    
                <rtexprvalue>false</rtexprvalue>    
      </attribute> 
      <attribute>    
                <name>readonly</name>    
                <required>false</required>    
                <rtexprvalue>false</rtexprvalue>    
      </attribute>
</tag>
<tag>  
	 <name>customerOrganizationSelector</name>                              
	 <tag-class>com.yuqincar.tags.CustomerOrganizationSelectorTags</tag-class>
	  <attribute>    
                <name>name</name>    
                <required>true</required>    
                <rtexprvalue>true</rtexprvalue>    
      </attribute> 
</tag>   
</taglib>  