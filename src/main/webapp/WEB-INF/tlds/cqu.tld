                <name>name</name>    
                <required>true</required>    
                <rtexprvalue>false</rtexprvalue>    
      </attribute> 
	 <attribute>    
                <name>driverOnly</name>    
                <required>false</required>    
                <rtexprvalue>false</rtexprvalue>    
      </attribute>
      <attribute>    
                <name>departments</name>    
                <required>false</required>    
                <rtexprvalue>false</rtexprvalue>    
      </attribute>
      <attribute>    
                <name>readonly</name>    
                <required>false</required>    
                <rtexprvalue>false</rtexprvalue>    
      </attribute>
      
</tag> 
<tag>  
	 <name>carSelector</name>                             
	 <tag-class>com.yuqincar.tags.CarSelectorTags</tag-class>
